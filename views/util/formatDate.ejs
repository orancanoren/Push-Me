<%
var months = [
  "Jan", "Feb", "Mar", "Apr", "May", "June",
  "July", "Aug", "Sep", "Oct", "Nov", "Dec"]
formatDate = function(date) {
  var formattedDate = "";
  formattedDate += date.getDate() + "-";
  formattedDate += months[date.getMonth()] + "-";
  formattedDate += date.getFullYear();
  return formattedDate;
}
%>
